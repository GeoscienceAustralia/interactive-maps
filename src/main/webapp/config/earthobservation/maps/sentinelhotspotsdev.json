		<Rule>
			<Name>Small Scale</Name>
			<Title>Hotspots detected from a satellite overpass within the last 2 hours</Title>
			<ogc:Filter>
				<ogc:And>
					<ogc:PropertyIsLessThanOrEqualTo>
						<ogc:PropertyName>age_hours</ogc:PropertyName>
						<ogc:Literal>2</ogc:Literal>
					</ogc:PropertyIsLessThanOrEqualTo> 
					<ogc:PropertyIsNotEqualTo>				
						<ogc:PropertyName>sensor</ogc:PropertyName>
						<ogc:Literal>Himawari-8</ogc:Literal>
					</ogc:PropertyIsNotEqualTo>
				</ogc:And>
			</ogc:Filter>
			<MinScaleDenominator>90001</MinScaleDenominator>
			<PointSymbolizer>
			<Graphic>
			<ExternalGraphic>
			<OnlineResource xlink:type="simple" xlink:href="http://localhost:8080/hotspots/ModisFire-Red.png"/>
